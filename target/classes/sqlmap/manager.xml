<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="Manager">
    	<resultMap id="result" type="Manager">
    		<result property="gid" column="gid"/>
    		<result property="gpass" column="gpass"/>
    	</resultMap>
    	<select id="searchId" parameterType="String" resultType="String">
    		select gid from manager where gid=#{gid}
    	</select>
    	<select id="searchPass" parameterType="String" resultType="String">
    		select gpass from manager where gid=#{gid}
    	</select>
    
</mapper>